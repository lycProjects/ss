*{
    margin: 0;
    padding: 0;
}
body{
font-family: "Microsoft YaHei";
}
.s_search{
	width:1050px;
	margin: 0 auto;
	overflow: hidden;
	margin-bottom: 20px;
}
 .s_search .search{
    height: 40px;
    line-height: 40px;
    background: url(../img/sousuo.png) no-repeat left center;
    text-indent: 30px; 
    color: rgb(255,103,0);
    font-size: 20px;
    border-bottom: 2px solid rgb(255,103,0);
    margin-bottom: 10px;
}
.s_serch_content_result{
	width:1050px;
	font-size: 16px;
	height:30px;
	line-height:30px ;
	float: left;
	margin-bottom: 10px
}
.s_serch_content_result #keywords{
	color:rgb(255,103,0);
}

.s_serch_content_result #result{
	color:rgb(255,103,0);
}

.s_serch_content_info{
	width: 1050px;
	height: 910px;
}
.s_serch_content_info ul li{
    width: 1050px;
    height: 130px;
    overflow: hidden;
    background: url(../img/qmjsline.png) bottom repeat-x;
    padding-bottom: 20px;
    margin-top: 16px;
}
.list_title{
    width: 1050px;
    height:25px;
    overflow: hidden;
}
.s_serch_content_info ul li a.zxtitle {
    display: block;
    height: 25px;
    line-height:25px ;
    font-size: 18px;
    color:#333333;
    overflow: hidden;
    margin-top: 5px;
    margin-bottom: 10px;
}

.s_serch_content_info ul li a:hover{
	text-decoration: underline;
	color: rgb(255,103,0);
}
a {
    text-decoration: none;
}

.s_serch_content_info ul li p{
    width: 1050px;
    color: #999999;
    height:67px;
    line-height: 23px;
    font-size: 14px;
    overflow: hidden;
}

}
.lifoot {
    width: 1050px;
    height: auto;
    display: block;
    overflow: hidden;
}
span.litime {
    color: #999999;
    font-size: 14px;
    float: right;
    margin-top: -12px ;
    margin-right:50px ;
}

.s_serch_content_info .sturn .li_turn{
	width: 150px;
	height: auto;
	float: right;
	margin-right: 37px;
	float: right; 
	margin-right: 126px; 
	margin-top: 24px;
}
.s_serch_content_info .sturn{
	width: 100%;
	height: 50px;
	line-height:50px;
}
.sturn a {
 /*    margin-right: 5px; */
    border: 1px solid #e6e6e6;
    color: #636363;
    font-size: 12px;
    padding: 2px 8px;
    text-decoration: none;
    border-radius: 2px;
}
.sturn a:hover{
	background-color: rgb(255,103,0);
	color: #ffffff;
	border: none
	
}
.s_serch_content .searchSelect{
	font-family: "Microsoft YaHei", Tahoma, sans-serif, simsun;
    display: inline-block;
    width: 120px;
    height: 32px;
    padding: 2px;
    line-height: 28px;
    border-radius: 4px;
    float: left;
    opacity: 0.6;
    color: black;
}
.s_serch_content .searchInput{
	margin-left: 6px;
	width: 420px;
    height: 32px;
    float: left;
    border: 1px solid #A9A9A9;
    padding: 0 5px;
    margin-right: 6px;
    font-size: 12px;
    line-height: 30px;
    vertical-align: middle;
    color: #7e7e7e;
    -webkit-appearance: none;
    border-radius: 4px;
    opacity: 0.6;
}
.s_serch_content .searchblock1{
	
    margin-bottom: 4px;
	
}
.s_serch_content .searchp{
	background: url(../img/qmjsline.png) bottom repeat-x;
    width: 1050px;
    height: 1px;
    float: left;
}
.list-b li {
	padding-left: 15px;
	line-height: 34px;
	height: 34px;
	margin-bottom: 5px;
	overflow: hidden;
	font-size: 14px;
	background: url(../img/icon.png) no-repeat 0 -876.5px;
	margin-left: 40px;
}
.list-b a {
	color: #333;
	
}

.list-b a:hover,
.list-b a:active {
	color: rgb(255, 103, 0);
	text-decoration: underline;
}
.searchblock1 .mybutton51{
	float: left;
    width: 68px;
    height: 31px;

    background-color: rgb(255,103,0);
    border: none;
    color:white;
    border-radius: 4px;
	
}
.pagination>.active>a, .pagination>.active>a:focus, .pagination>.active>a:hover, .pagination>.active>span, .pagination>.active>span:focus, .pagination>.active>span:hover{
	background-color: #FF6700;
    border-color: #FF6700;
   /*  color: #FF6700; */
}
.pagination>li>a, .pagination>li>span{
	    color: #777;
}
.pagination>li>a:hover{
	color: #FF6700;
}
