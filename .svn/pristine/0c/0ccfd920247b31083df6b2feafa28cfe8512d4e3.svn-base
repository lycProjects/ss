* {
 word-break:break-all
}
a,abbr,acronym,address,applet,article,aside,audio,b,big,blockquote,body,canvas,caption,center,cite,code,dd,del,details,dfn,div,dl,dt,em,embed,fieldset,figcaption,figure,footer,form,h1,h2,h3,h4,h5,h6,header,hgroup,html,i,iframe,img,ins,kbd,label,legend,li,mark,menu,nav,object,ol,output,p,pre,q,ruby,s,samp,section,small,span,strike,strong,sub,summary,sup,table,tbody,td,tfoot,th,thead,time,tr,tt,u,ul,var,video {
 margin:0;
 padding:0
}
body {
 width:100%;
 font-size:14px;
 padding:0;
 margin:0 auto;
 overflow:-moz-scrollbars-vertical;
 background-color:#fff;
 color:#525252;
 font-family:"Microsoft YaHei",Arial,Helvetica,sans-serif
}
article,aside,dialog,figure,footer,header,menu,nav,section {
 display:block;
 word-break:break-all
}
img {
 border:none;
 vertical-align:text-bottom
}
dd,dl,dt,form,h1,h2,h3,h4,h5,h6,li,p,ul {
 margin:0;
 padding:0;
 word-break:break-all
}
div,label,span,table,td,th {
 word-break:break-all;
}
li,ul {
 list-style:none
}
h1 {
 font-size:14px;
 color:#005688
}
input {
 outline:0
}
a {
 color:#0084C3;
 text-decoration:none
}
a:hover {
 color:#000;
 text-decoration:none
}
a.more {
 color:#0084C3;
 text-decoration:none
}
a.detail {
 color:#FF5F10
}
a.white {
 color:#fff!important
}
a.white:hover {
 color:#fff!important
}
textarea {
 width:100%;
 height:100px;
 padding:0;
 margin:0 auto
}
h1,h2,h3,h4,h5 {
 color:#222;
 text-align:center
}
h1 {
 font-size:22px;
 line-height:24px;
 padding:10px 0
}
h2 {
 font-size:18px;
 line-height:20px
}
h3 {
 font-size:30px;
 line-height:24px
}
h4 {
 font-size:14px;
 line-height:16px
}
h5 {
 font-size:12px;
 line-height:14px
}
.clearfix:before {
 content:"";
 display:table
}
.clearfix:after {
 clear:both
}
.clearfix {
 zoom:1;
 clear:both
}
.h16 {
 height:16px
}
.lr {
 overflow:auto;
 zoom:1
}
.lr .l {
 float:left
}
.lr .m {
 float:left
}
.lr .mm {
 float:left
}
.lr .r {
 float:right
}
.lr,.lr .l,.lr .m,.lr .mm,.lr .r {
 -webkit-box-sizing:border-box!important;
 -moz-box-sizing:border-box!important;
 box-sizing:border-box!important
}
.st-w {
 width:500px;
 margin:0 auto;
 -webkit-box-sizing:border-box;
 -moz-box-sizing:border-box;
 box-sizing:border-box
}
@media (min-width:1200px) {
 .st-w {
  width:1050px;
  margin:0 auto
 }
}

.pagelist {
 width:auto;
 padding:12px 0;
 text-align:center;
 padding-left:1px;
 overflow:auto;
 zoom:1
}
.pagelist .p-total {
 display:none
}
.pagelist .pactive,.pagelist a,.pagelist b,.pagelist span {
 z-index:1;
 padding:0 10px;
 height:36px;
 line-height:36px;
 vertical-align:middle;
 display:inline-block;
 font-size:14px;
 color:#979797!important;
 text-align:center;
 background-color:#fff;
 border:1px solid #fff;
 border-radius:4px
}
.pagelist b {
 font-weight:400
}
.pagelist span:first-child {
 display:none
}
.pagelist span {
 cursor:auto
}
.pagelist span i {
 font-weight:bolder;
 font-style:normal;
 color:#169EB6
}
.pagelist .pactive,.pagelist a:hover,.pagelist b {
 z-index:2!important;
 color:#fff!important;
 text-decoration:none!important;
 background-color:#ff8712!important;
 border:1px solid #ff8712;
 border-radius:4px
}
.pagelist .pactive {
 font-weight:700
}
.pagelist .pagetext {
 width:35px;
 height:24px;
 line-height:22px;
 vertical-align:middle;
 padding:0;
 border:1px solid #B3B3B3;
 padding-left:2px;
 border-radius:4px;
 border-top-right-radius:0;
 border-bottom-right-radius:0
}
.pagelist .pagebtn {
 cursor:pointer;
 width:35px;
 height:26px;
 vertical-align:middle;
 border:1px solid #B3B3B3;
 border-left:none;
 background-color:#EAEAEA;
 color:#717171;
 border-top-right-radius:4px;
 border-bottom-right-radius:4px
}
.pagelist .pagebtn:hover {
 color:#0E8290;
 background-color:#DCEFF1
}
.pagelist-center {
 text-align:center!important
}
.pagelist-right {
 text-align:right!important
}
