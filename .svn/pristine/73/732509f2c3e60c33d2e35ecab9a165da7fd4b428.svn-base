* {
	margin: 0;
	padding: 0;
}

#s-market-body {
	width: 1050px;
	height: 778px;
	margin: 0 auto;
}

.s-market-head {
	width: 1050px;
	height: 40px;
	margin-bottom: 10px;
	margin: 0 auto;
	margin-bottom: 10px;
}

.breadcrumb .bdli a {
	color: rgb(255, 103, 0);
}

.s-market-content {
	width: 1050px;
	height: 722px;
	position: relative;
	border: solid 1px #dcdcdc;
	background-color: #fff;
}
.s-market-content .changeul {
	
    margin-top: 16px;
}
.list-b li {
	padding-left: 15px;
	line-height: 34px;
	height: 34px;
	margin-bottom: 5px;
	overflow: hidden;
	font-size: 14px;
	background: url(../img/icon01.png) no-repeat 0 -877.5px;
}
.s-market-content .pagination li a{
	color: black;
}
.s-market-content .pagination li a:HOVER{
	color: rgb(255,103,0);
}
.list-b a {
	color: #333;
}

.list-b a:hover,
.list-b a:active {
	color: rgb(255, 103, 0);
	text-decoration: underline;
}
.changeol {
	background: #f6f6f6;
    border: solid 1px #dcdcdc;
    border-radius: 0px!important;
    border-bottom: 2px solid rgb(255,103,0);
}
.s-market-content .s-market-top-right {
	width: 450px;
	height: 300px;
	right: 29px;
    top: 23px;
	overflow: hidden;
	position: absolute;
}

.carousel-caption a {
	color: #F7F7F7;
}
.carousel-caption a:hover {
    color: rgb(255, 103, 0);
}
.breadcrumb {
	padding: 8px 8px;
}
a{
	color: #777;
	
}
A:HOVER {
	color: #FF6700;
}