.wendyslide {
 width:100%;
 background-color:#fff;
 clear:both;
 margin:0 auto;
 text-align:center;
 position:relative
}
.wendyslide ul {
 overflow:auto;
 zoom:1;
 position:relative;
 padding:0;
 margin:0 auto
}
.wendyslide ul li {
 padding:0;
 margin:0;
 float:left
}
.wendyslide ul img {
 vertical-align:top
}
.wendyslide .smenu {
 height:12px;
 position:relative;
 top:-24px;
 zoom:1;
 z-index:8000;
 padding:0;
 margin:0 auto
}
.wendyslide .smenu .smunormal,.wendyslide .smenu a {
 background-color:#D2D2D2;
 border:1px solid #D2D2D2;
 color:#D2D2D2;
 display:inline-block;
 margin-right:2px;
 width:10px;
 height:10px;
 line-height:10px;
 font-size:10px;
 -webkit-border-radius:50%;
 -moz-border-radius:50%;
 -ms-border-radius:50%;
 -o-border-radius:50%;
 border-radius:50%
}
.wendyslide .smenu .smucurr,.wendyslide .smenu a:hover {
 color:#FC7F2B;
 background-color:#FC7F2B;
 border:1px solid #FC7F2B
}
.wendyslide .snext,.wendyslide .sprev {
 z-index:1000;
 width:47px;
 height:48px;
 display:none;
 position:absolute;
 top:50%;
 margin-top:-19px;
 cursor:pointer;
 opacity:.5;
 -webkit-transition:opacity .5s;
 -moz-transition:opacity .5s;
 -ms-transition:opacity .5s;
 -o-transition:opacity .5s;
 transition:opacity .5s
}
.wendyslide .snext a,.wendyslide .sprev a {
 width:47px;
 height:48px;
 display:block;
 background:transparent url(images/arrow-pn.png) no-repeat;
 background-position:0 0
}
.wendyslide .snext a {
 background-position:100% 0
}
.wendyslide .snext:hover,.wendyslide .sprev:hover {
 opacity:1
}
.wendyslide .sprev {
 left:10px
}
.wendyslide .snext {
 right:10px
}
