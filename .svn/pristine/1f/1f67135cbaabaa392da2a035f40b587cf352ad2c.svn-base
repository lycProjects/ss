<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Sansu</display-name>
  <welcome-file-list>
    <welcome-file>b.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>initServlet</servlet-name>
    <servlet-class>com.Sansu.web.InitServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>initServlet</servlet-name>
    <url-pattern>/init</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>indexServlet</servlet-name>
    <servlet-class>com.Sansu.web.IndexServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>indexServlet</servlet-name>
    <url-pattern>/goIndex</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>newsServlet</servlet-name>
    <servlet-class>com.Sansu.web.NewsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>newsServlet</servlet-name>
    <url-pattern>/news</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>commentServlet</servlet-name>
    <servlet-class>com.Sansu.web.CommentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>commentServlet</servlet-name>
    <url-pattern>/comment</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>userServlet</servlet-name>
    <servlet-class>com.Sansu.web.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userServlet</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>linkServlet</servlet-name>
    <servlet-class>com.Sansu.web.LinkServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>linkServlet</servlet-name>
    <url-pattern>/link</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>newsTypeServlet</servlet-name>
    <servlet-class>com.Sansu.web.NewsTypeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>newsTypeServlet</servlet-name>
    <url-pattern>/newsType</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>imageUpload</servlet-name>
    <servlet-class>com.Sansu.web.ImageUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>imageUpload</servlet-name>
    <url-pattern>/imageUpload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>fileUpload</servlet-name>
    <servlet-class>com.Sansu.web.FileUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fileUpload</servlet-name>
    <url-pattern>/fileUpload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BusinessServlet</servlet-name>
    <servlet-class>com.Sansu.web.BusinessServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BusinessServlet</servlet-name>
    <url-pattern>/business</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>UrlRewriteFilter</filter-name>
    <filter-class>  
            org.tuckey.web.filters.urlrewrite.UrlRewriteFilter  
        </filter-class>
  </filter>
  <filter-mapping>
    <filter-name>UrlRewriteFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>regServlet</servlet-name>
    <servlet-class>RegServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>regServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>phoneCodeServlet</servlet-name>
    <servlet-class>PhoneCodeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>phoneCodeServlet</servlet-name>
    <url-pattern>/phonecode</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Curioservlet</servlet-name>
    <servlet-class>com.Sansu.web.Curioservlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Curioservlet</servlet-name>
    <url-pattern>/curio</url-pattern>
  </servlet-mapping>
  <!-- <servlet>
    <servlet-name>CurioServiceServlet</servlet-name>
    <servlet-class>com.Sansu.web.CurioServiceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CurioServiceServlet</servlet-name>
    <url-pattern>/curioshow</url-pattern>
  </servlet-mapping> -->
</web-app>